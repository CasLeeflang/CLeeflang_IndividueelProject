@model IEnumerable<CLeeflang_IndividueelProject.Models.Reservation.ReservationOverviewModel>;

<table>
    <tr>
        <th>Date:</th>
        <th>Business:</th>
        <th>Start Time:</th>
        <th>End Time:</th>
    </tr>

    @foreach (var reservation in Model)
    {
       
        <tr>
            <td>@reservation.Date</td>
            <td>@reservation.BusinessName</td>
            <td>@reservation.StartTime</td>
            <td>@reservation.EndTime</td>
        </tr>
    }
</table>